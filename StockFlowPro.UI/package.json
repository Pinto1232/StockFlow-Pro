{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:docker": "cross-env VITE_API_BASE_URL=http://localhost:5000/api VITE_WS_URL=ws://localhost:5000 vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "test": "vitest",
    "test:run": "vitest run",
    "test:integration": "vitest run src/architecture/__tests__/integration.test.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@microsoft/signalr": "^8.0.7",
    "@tanstack/react-query": "^5.83.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "lucide-react": "^0.525.0",
    "moment": "^2.30.1",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-datetime": "^3.3.1",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
