{
  "yaml.schemas": {},
  "yaml.schemaStore.enable": false,
  "yaml.validate": false,
  "files.associations": {
    ".github/ISSUE_TEMPLATE/config.yml": "plaintext",
    ".github/workflows/project-automation.yml": "plaintext",
    ".github/workflows/*.yml": "plaintext",
    ".github/workflows/*.yaml": "plaintext"
  },
  "redhat.telemetry.enabled": false,
  "zencoder.enableRepoIndexing": true,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "css.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "less.lint.unknownAtRules": "ignore",
  "codium.codeCompletion.enable": false,
  "window.autoDetectColorScheme": true,
  "workbench.colorTheme": "Default Dark Modern",
  "chat.mcp.autostart": "never"
}