{
  "dashboard": {
    "id": null,
    "title": "StockFlow Pro - Comprehensive Monitoring",
    "tags": ["stockflow", "inventory", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Performance Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Database Connection Pool",
        "type": "graph",
        "targets": [
          {
            "expr": "sqlserver_connections_active",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "sqlserver_connections_total",
            "legendFormat": "Total Connections"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "yAxes": [
          {
            "label": "Connections",
            "min": 0
          }
        ]
      },
      {
        "id": 3,
        "title": "System Resource Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_cpu_seconds_total",
            "legendFormat": "CPU Usage"
          },
          {
            "expr": "process_resident_memory_bytes",
            "legendFormat": "Memory Usage"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Inventory Metrics",
        "type": "table",
        "targets": [
          {
            "expr": "stockflow_products_total",
            "legendFormat": "Total Products"
          },
          {
            "expr": "stockflow_products_low_stock",
            "legendFormat": "Low Stock Items"
          },
          {
            "expr": "stockflow_products_out_of_stock",
            "legendFormat": "Out of Stock Items"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "User Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "stockflow_users_active",
            "legendFormat": "Active Users"
          },
          {
            "expr": "stockflow_auth_attempts_total",
            "legendFormat": "Authentication Attempts"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Error Rate",
        "type": "singlestat",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 24
        },
        "thresholds": "1,5",
        "colorBackground": true
      },
      {
        "id": 7,
        "title": "Response Time P95",
        "type": "singlestat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Response Time"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 24
        },
        "thresholds": "0.5,1",
        "colorBackground": true,
        "format": "s"
      },
      {
        "id": 8,
        "title": "Database Query Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "sqlserver_query_duration_seconds",
            "legendFormat": "Query Duration"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 9,
        "title": "Recent Alerts",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"stockflow-api\"} |= \"ERROR\" or \"WARN\"",
            "legendFormat": "Application Logs"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        }
      }
    ],
